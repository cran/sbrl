GSL_CFLAGS = @GSL_CFLAGS@
GSL_LIBS = @GSL_LIBS@
GMP_CFLAGS = @GMP_CFLAGS@
GMP_LIBS = @GMP_LIBS@

PKG_LIBS= $(GSL_LIBS) -L/usr/lib64 $(GMP_LIBS)
PKG_CFLAGS= -DGMP -I../inst/include $(GSL_CFLAGS) -I/usr/include $(GMP_CFLAGS)
PKG_CXXFLAGS= -DGMP -I../inst/include $(GSL_CFLAGS) -I/usr/include $(GMP_CFLAGS)
